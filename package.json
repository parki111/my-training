{
  "name": "fhss-starter-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "build:prod": "npm run prisma:generate && ng build --configuration=production",
    "start:prod": "node dist/server/server.mjs",
    "test": "vitest run",
    "test:client": "vitest run --project client",
    "test:server": "vitest run --project server",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:gui": "vitest --ui",
    "prisma:generate": "prisma generate --no-hints",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "e2e": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "e2e:codegen": "npx playwright codegen localhost:4200",
    "e2e:report": "npx playwright show-report e2e/playwright/playwright-report",
    "lint": "ng lint --fix",
    "git:clean": "git fetch -p && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs git branch -d",
    "check": "ng lint && npm run test && npm run e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/platform-server": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@angular/ssr": "^19.2.15",
    "@faker-js/faker": "^9.8.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/static": "^8.2.0",
    "@fhss-web-team/backend-utils": "^4.0.16",
    "@fhss-web-team/frontend-utils": "^4.0.3",
    "@node-saml/node-saml": "^5.0.1",
    "@prisma/client": "^6.10.0",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "cron": "^4.3.2",
    "dotenv": "^16.5.0",
    "fastify": "^5.4.0",
    "jose": "^6.0.11",
    "rxjs": "~7.8.0",
    "superjson": "^2.2.2",
    "tslib": "^2.3.0",
    "zod": "^3.25.67",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.19.0",
    "@analogjs/vite-plugin-angular": "^1.19.0",
    "@analogjs/vitest-angular": "^1.19.0",
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@nx/angular": "^21.2.1",
    "@nx/vite": "^21.0.0",
    "@playwright/test": "1.54.1",
    "@types/express": "^4.17.23",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.19.112",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "angular-eslint": "^19.8.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jasmine-core": "~5.6.0",
    "jsdom": "^22.0.0",
    "playwright-ng-schematics": "^2.1.0",
    "prettier": "^3.6.2",
    "prisma": "^6.10.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^3.0.0"
  },
  "prisma": {
    "schema": "./prisma"
  }
}
